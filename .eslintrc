{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "object-curly-newline": "warn",
    "import/order": "warn",
    "no-useless-return": "warn",
    "prefer-const": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src", "./src/style"],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
