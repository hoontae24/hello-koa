{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "consistent-return": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src", "./src/style"],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
